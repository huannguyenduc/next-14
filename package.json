{
  "name": "wellnez-admin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "clean-up": "rm -rf .git && git init && yarn husky:setup",
    "husky:setup": "npx husky-init && yarn husky:commitlint && yarn husky:prettier && yarn husky:lint",
    "husky:commitlint": "npx husky add .husky/commit-msg 'npx --no-install commitlint --edit'",
    "husky:prettier": "npx husky set .husky/pre-commit 'yarn prettier'",
    "husky:lint": "npx husky add .husky/pre-commit 'yarn lint'",
    "prettier": "prettier --write '**/*.{js,ts,tsx,json}'",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@svgr/webpack": "^8.1.0",
    "@types/node": "20.14.9",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "antd": "^5.11.5",
    "axios": "^1.7.3",
    "dayjs": "^1.11.12",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.4",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.21",
    "next": "14.2.4",
    "next-intl": "^3.17.2",
    "next-themes": "^0.3.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-verification-input": "2.0.3",
    "styled-components": "^6.1.12",
    "swr": "^2.2.5",
    "typescript": "5.4.3",
    "zustand": "^4.5.4"
  },
  "devDependencies": {
    "@ant-design/icons": "^5.4.0",
    "@ant-design/nextjs-registry": "^1.0.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/js-cookie": "^3",
    "@types/lodash": "^4",
    "@types/react-slick": "^0",
    "@types/slick-carousel": "^1",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.10.0",
    "antd-mask-input": "^2.0.7",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "commitlint": "^19.3.0",
    "echarts": "^5.5.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.7",
    "nextjs-toploader": "^1.6.12",
    "prettier": "^3.3.3",
    "react-infinite-scroll-component": "^6.1.0",
    "react-responsive": "^10.0.0",
    "react-slick": "^0.30.2",
    "slick-carousel": "^1.8.1",
    "typeface-montserrat": "^1.1.13"
  },
  "packageManager": "yarn@4.4.1"
}
